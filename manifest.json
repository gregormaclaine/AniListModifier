{
  "name": "AniList Modifier",
  "description": "Add scores to AniList activity feed",
  "version": "1.0.2",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["https://anilist.co/*"],
      "js": ["injection.js"],
      "css": []
    }
  ],
  "icons": {
    "48": "icons/48.png",
    "128": "icons/128.png",
    "256": "icons/256.png",
    "512": "icons/512.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}
